{
  "moduleNameMapper": {
    "\\.(avif|css|jpg|png|svg|webm|webp|woff|woff2)$": "<rootDir>/__mocks__/empty.js"
  },
  "testEnvironment": "jsdom",
  "setupFilesAfterEnv": ["<rootDir>/jest-setup.js"],
  "collectCoverageFrom": ["**/*.{ts,tsx,js,jsx}", "!**/node_modules/**"],
  "coveragePathIgnorePatterns": ["<rootDir>/node_modules/**", "<rootDir>/src/types/**"],
  "coverageThreshold": {
    "global": {
      "statements": 0.5
    }
  }
}
